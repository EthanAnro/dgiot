    "/notification": {
      "get": {
        "description": "告警列表",
        "parameters": [
          {
            "name": "productid",
            "in": "query",
            "type": "string",
            "default": "all",
            "description": "all,产品id 设备类型"
          },
          {
            "name": "order",
            "in": "query",
            "type": "string",
            "default": "createdAt",
            "description": "排序方式, 负为降序, eg: score,-name"
          },
          {
            "name": "limit",
            "in": "query",
            "type": "string",
            "default": "10",
            "description": "limit, 显示多少条记录，显示100条记录 eg:100"
          },
          {
            "name": "skip",
            "in": "query",
            "type": "string",
            "default": "0",
            "description": "skip,第几页"
          },
          {
            "name": "isprocess",
            "in": "query",
            "type": "string",
            "default": "all",
            "description": "all, 0 未处理, 1 已处理, 2 误报 是否处理"
          }
        ],
        "responses": {
          "200": {
            "description": "Returns operation status"
          },
          "400": {
            "description": "Bad Request"
          },
          "403": {
            "description": "Forbidden"
          },
          "500": {
            "description": "Server Internal error"
          }
        },
        "summary": "告警列表",
        "tags": [
          "Wechat"
        ]
      }
    },
