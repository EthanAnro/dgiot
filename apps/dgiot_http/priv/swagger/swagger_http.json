    "/notification": {
      "get": {
        "description": "告警列表",
        "parameters": [
          {
            "name": "productid",
            "in": "query",
            "type": "string",
            "default": "all",
            "description": "all,产品id 设备类型"
          },
          {
            "name": "order",
            "in": "query",
            "type": "string",
            "default": "createdAt",
            "description": "排序方式, 负为降序, eg: score,-name"
          },
          {
            "name": "limit",
            "in": "query",
            "type": "string",
            "default": "10",
            "description": "limit, 显示多少条记录，显示100条记录 eg:100"
          },
          {
            "name": "skip",
            "in": "query",
            "type": "string",
            "default": "0",
            "description": "skip,第几页"
          },
          {
            "name": "isprocess",
            "in": "query",
            "type": "string",
            "default": "all",
            "description": "all, 0 未处理, 1 已处理, 2 误报 是否处理"
          }
        ],
        "responses": {
          "200": {
            "description": "Returns operation status"
          },
          "400": {
            "description": "Bad Request"
          },
          "403": {
            "description": "Forbidden"
          },
          "500": {
            "description": "Server Internal error"
          }
        },
        "summary": "告警列表",
        "tags": [
          "Wechat"
        ]
      }
    },
    "/sendsubscribe": {
      "post": {
        "basePath": "/",
        "description": "发送订阅消息",
        "parameters": [
          {
            "in": "body",
            "name": "data",
            "schema": {
              "type": "object",
              "properties": {
                "templateid": {
                  "type": "string",
                  "required": true,
                  "example": "9Fmc0vtA7vnh_HtoVtXJy6Y4_GgjqM821MU8HzojUy8",
                  "description": "模板id"
                },
                "page": {
                  "type": "string",
                  "example": "/index",
                  "description": "跳转页面"
                },
                "miniprogramstate": {
                  "type": "string",
                  "required": true,
                  "example": "formal",
                  "description": "小程序类型"
                },
                "lang": {
                  "type": "string",
                  "required": true,
                  "example": "zh_CN",
                  "description": "语言类型"
                },
                "data": {
                  "type": "object",
                  "required": true,
                  "example": "{}",
                  "description": "模板内容"
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": ""
          },
          "404": {
            "description": ""
          }
        },
        "summary": "发送订阅消息",
        "tags": [
          "Wechat"
        ]
      }
    },
    "/sendemail": {
      "post": {
        "basePath": "/",
        "description": "发送邮件",
        "parameters": [
          {
            "in": "body",
            "name": "data",
            "schema": {
              "type": "object",
              "properties": {
                "from": {
                  "type": "string",
                  "required": true,
                  "example": "18257190166@163.com",
                  "description": "发件人邮箱"
                },
                "to": {
                  "type": "array",
                  "example": ["463544084@qq.com"],
                  "description": "收件人邮箱"
                },
                "subject": {
                  "type": "string",
                  "required": true,
                  "example": "测试邮件",
                  "description": "主题"
                },
                "fromdes": {
                  "type": "string",
                  "required": true,
                  "example": "18257190166@163.com",
                  "description": "发件人描述"
                },
                "todes": {
                  "type": "string",
                  "required": true,
                  "example": "463544084@qq.com",
                  "description": "收件人描述"
                },
                "data": {
                  "type": "string",
                  "required": true,
                  "example": "发件内容",
                  "description": "发件内容"
                },
                "relay": {
                  "type": "string",
                  "required": true,
                  "example": "smtp.163.com",
                  "description": "中转服务"
                },
                "username": {
                  "type": "string",
                  "required": true,
                  "example": "18257190166@163.com",
                  "description": "发件人邮箱账户"
                },
                "password": {
                  "type": "string",
                  "required": true,
                  "example": "ALRFYEVAFSITDXSX",
                  "description": "密钥"
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": ""
          },
          "404": {
            "description": ""
          }
        },
        "summary": "发送邮件",
        "tags": [
          "Wechat"
        ]
      }
    },
