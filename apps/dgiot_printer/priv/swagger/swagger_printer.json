{
  "tags": [
    {
      "name": "Printer",
      "description": "打印机管理"
    }
  ],
  "definitions": {
  },
  "paths": {
    "/printer": {
      "put": {
        "summary": "修改打印任务",
        "description": "修改打印任务",
        "parameters": [
          {
            "in": "body",
            "name": "data",
            "description": "打印",
            "required": true,
            "schema": {
              "type": "object",
              "properties": {
                "devaddr": {
                  "type": "string",
                  "example":"28a68a908d8c",
                  "description": "打印机设备地址"
                },
                "template": {
                  "type": "string",
                  "example":"ipjnGiLSt7",
                  "description": "打印模板类型"
                },
                "enable": {
                  "description": "控制动作",
                  "type": "boolean",
                  "enum": [
                    true,
                    false
                  ],
                  "example":true
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Returns success"
          },
          "400": {
            "description": "Bad Request"
          },
          "401": {
            "description": "Unauthorized"
          },
          "403": {
            "description": "Forbidden"
          },
          "500": {
            "description": "Server Internal error"
          }
        },
        "tags": [
          "Printer"
        ]
      },
      "get": {
        "summary": "获取打印任务",
        "description": "获取打印任务",
        "parameters": [
          {
            "name": "type",
            "in": "query",
            "type": "string",
            "enum": [
              "ALL",
              "TOPO",
              "WORD"
            ],
            "default": "ALL",
            "description": "打印模板类型  ALL：所有类型，TOPO:组态打印模板, WORD: word文档打印模板 "
          },
          {
            "name": "devaddr",
            "in": "query",
            "type": "string",
            "default": "28a68a908d8c",
            "description": "视频设备地址"
          }
        ],
        "responses": {
          "200": {
            "description": "Returns operation status"
          },
          "400": {
            "description": "Bad Request"
          },
          "401": {
            "description": "Unauthorized"
          },
          "403": {
            "description": "Forbidden"
          },
          "500": {
            "description": "Server Internal error"
          }
        },
        "tags": [
          "Printer"
        ]
      }
    }
  }
